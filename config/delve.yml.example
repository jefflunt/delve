# delve configuration file
#
# copy this file to delve.yml and fill in your credentials.
# do not commit delve.yml to source control.

confluence:
  # add each of your confluence instances here
  # the key is the hostname of the confluence instance
  "your-instance.atlassian.net":
    username: "your-email@example.com"
    api_token: "your-confluence-api-token"
    space_key: "YOUR_SPACE_KEY"
